      <ol id="testing-issues-user-<%= h user.id -%>" class="testing-issues <%= allowed_to_assign_staffed_issue_to(user) -%>">
        <% if @kanban.testing_issues_for(:user => user).empty? %>
        <%= render :partial => 'empty_issue' %>
        <% else %>
        <%= render(:partial => 'issue',
                   :collection => @kanban.testing_issues_for(:user => user),
                   :locals => { :limit => Setting['plugin_redmine_kanban']["panes"]["testing"]["limit"].to_i }) %>
        <% end %>
      </ol>

