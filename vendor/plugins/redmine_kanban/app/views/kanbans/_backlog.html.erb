      <%= content_tag(:h3, l(:kanban_text_backlog)) %>
      <ol id="backlog-issues">
        <% if @kanban.backlog_issues_for.empty? %>
        <%= render :partial => 'empty_issue' %>
        <% else %>
        <%= render :partial => 'grouped_issues', :object => @kanban.backlog_issues_for.group_by(&:priority) %>
        <% end %>
      </ol>
