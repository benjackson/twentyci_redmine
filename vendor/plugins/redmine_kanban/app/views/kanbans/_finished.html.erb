      <ol id="finished-issues-user-<%= h user.id -%>" class="finished-issues <%= allowed_to_assign_staffed_issue_to(user) -%>">
        <% if @kanban.finished_issues_for(:user => user).blank? %>
        <%= render :partial => 'empty_issue' %>
        <% else %>
        <%= render(:partial => 'issue', :collection => @kanban.finished_issues_for(:user => user)) %>
        <% end %>
      </ol>

